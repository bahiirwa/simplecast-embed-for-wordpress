{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "simplecast/embed-block",
  "version": "0.1.0",
  "title": "Simplecast Podcast Embed for WordPress",
  "category": "media",
  "description": "Simplecast Embed for WordPressDisplay content using a shortcode to insert in a page or post.",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full",
      "center",
      "left",
      "right"
    ],
    "inserter": true
  },
  "keywords": [
    "Simplecast",
    "Podcast",
    "WordPress",
    "Embed"
  ],
  "attributes": {
    "link": {
      "type": "string",
      "default": "https://cat-jokes-pawdcast.simplecast.com/episodes/episode-1-matttest"
    },
    "isDark": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}